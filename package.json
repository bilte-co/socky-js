{
  "name": "socky",
  "version": "0.2.4",
  "description": "JavaScript SDK for the Socky Flights API",
  "license": "Apache-2.0",
  "type": "module",
  "private": false,
  "sideEffects": false,
  "engines": { "node": ">=18" },
  "repository": { "type": "git", "url": "git+https://github.com/bilte-co/socky-js.git" },
  "bugs": { "url": "https://github.com/bilte-co/socky-js/issues" },
  "homepage": "https://github.com/bilte-co/socky-js#readme",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "default": "./dist/index.js"
    },
    "./package.json": "./package.json"
  },
  "files": ["dist", "README.md", "LICENSE"],
  "publishConfig": { "access": "public" },
  "scripts": {
    "prepare": "pnpm -v > /dev/null",
    "clean": "rimraf dist",
    "build": "tsup",
    "dev": "tsup --watch",
    "lint": "eslint . --ext .ts",
    "typecheck": "tsc --noEmit",
    "prepack": "pnpm clean && pnpm build && pnpm typecheck",
    "release": "changeset version && pnpm install --no-frozen-lockfile && changeset publish",
    "changeset": "changeset",
    "test": "echo \"(no tests yet)\""
  },
  "devDependencies": {
    "@changesets/cli": "^2.27.9",
    "@typescript-eslint/eslint-plugin": "^8.8.0",
    "@typescript-eslint/parser": "^8.8.0",
    "@types/node": "^20.12.12",
    "eslint": "^9.12.0",
    "rimraf": "^6.0.1",
    "tsup": "^8.1.0",
    "typescript": "^5.6.3"
  }
}
